# Set variables
REGION=us-central1
REPOSITORY=nss
IMAGE=network-simulation-server

# Build
docker build -t $REGION-docker.pkg.dev/$GOOGLE_CLOUD_PROJECT/$REPOSITORY/$IMAGE:latest -f deployment/Dockerfile .

# Authenticate Docker
gcloud auth configure-docker $REGION-docker.pkg.dev

# Push
docker push $REGION-docker.pkg.dev/$GOOGLE_CLOUD_PROJECT/$REPOSITORY/$IMAGE:latest